<?xml version="1.0" encoding="utf-8"?>
<PRODUCTINFO>
 		<!-- 软件公司 及 软件名，需要与软件配合使用，用于读取 注册表 信息，一般由开发人员指定 -->
 		<PRODUCTINFO_COMPANY>Gihosoft</PRODUCTINFO_COMPANY>
 		<PRODUCTINFO_NAME>TubeGet</PRODUCTINFO_NAME>
 		
 		<!-- 软件显示的标题，Title为默认 ，中文使用 Title-ZH  -->
 		<Title>Update</Title>
 		<Title-ZH>升级</Title-ZH>
 		
 		<!-- 软件描述，Desc为默认 ，中文使用 Desc-ZH  -->
 		<Desc>Upgrade to dual or home versions and authorize login on more computers</Desc>
 		<Desc-ZH>升级成为双机版或家庭版在更多电脑授权登录</Desc-ZH>
 		
 		
 		<!-- 软件版本信息  -->
 		<Version>1.0.0</Version>
 		<!-- 软件是否免费  -->
 		<Free>False</Free>
 		
 		<!-- 软件可执行文件名，此参数可以不设置， 注意：不是安装包的名称， 而是安装后，双击打开运行文件 的名称   -->
 		<Exec_Name></Exec_Name>
 		<!-- 安装包的UUID 用于兼容老版本程序 的启动 -->
 		<Installer_UUID></Installer_UUID>
 		
 		<!-- 软件安装包下载地址   -->
 		<DownloadURL1></DownloadURL1>
 		<DownloadURL2></DownloadURL2>
		<DownloadURL3></DownloadURL3>
 		
 		<!-- 软件安装包的md5, 用于验证下载的安装包是否正确   -->
 		<FileMD5></FileMD5>
 		
 		<!-- 软件帮助链接页   -->
 		<HelpLink>https://www.tubegetpro.com</HelpLink>
 		
 		<!-- 软件的ICON，若配制了Icon_Data, 则此Icon_URL项可有可无 -->
 		<Icon_URL></Icon_URL>   
 		<Icon_Data>data:image/png;base64,</Icon_Data>
 		
 		<!-- UI设置相关， 默认不需要UI设置 -->
 		<!-- 背景 -->
 		<UIbgcolor>#FFF2F2</UIbgcolor>
 		<UIbgcolor-deep>#2e2929</UIbgcolor-deep>
 		<!-- 按钮 -->
 		<UIBtncolor>#d91e1f</UIBtncolor>  <!-- 按钮底色 -->
 		<UIBtnlightcolor>#ff0000</UIBtnlightcolor> <!-- 按钮底色亮色 鼠标移上去的颜色-->
 		<UIBtnTXTcolor>#ffffff</UIBtnTXTcolor> <!-- 按钮文本颜色 -->
 		<UIBtncolor-deep>#d91e1f</UIBtncolor-deep> <!--深色模式下 按钮底色 -->
 		<UIBtnlightcolor-deep>#ff0000</UIBtnlightcolor-deep> <!--深色模式下 按钮底色亮色 鼠标移上去的颜色-->
 		<UIBtnTXTcolor-deep>#b5b5b6</UIBtnTXTcolor-deep> <!--深色模式下 按钮文本颜色 -->
 		<UIBtnTXT>Update</UIBtnTXT> <!-- 按钮文本内容 -->
 		<UIBtnTXT-ZH>升级</UIBtnTXT-ZH> <!--中文 按钮文本内容 -->
 		
 </PRODUCTINFO>